var timeFormat="MM/DD/YYYY HH:mm",chartconfig={type:"line",data:{labels:[],datasets:[{label:"Profit",backgroundColor:"rgb(255, 99, 132)",borderColor:"rgb(255, 99, 132)",data:[],fill:!1},{label:"Principal",fill:!1,backgroundColor:"rgb(54, 162, 235)",borderColor:"rgb(54, 162, 235)",data:[]}]},options:{responsive:!0,title:{display:!0,text:"AllHYIPMonitor.com Calculator"},tooltips:{mode:"index"},hover:{mode:"nearest",intersect:!0},scales:{xAxes:[{type:"time",time:{parser:timeFormat,tooltipFormat:"ll HH:mm"},scaleLabel:{display:!0,labelString:"Date"}}],yAxes:[{display:!0,scaleLabel:{display:!0,labelString:"Amount"}}]}}};function chartData(e,t){creinv=[],cprofit=[];var a=new Date;a.setHours(0),a.setMinutes(0),a.setSeconds(0),a.setMilliseconds(0);var r=a.getTime(),s=new Date;s.setUTCHours(0),s.setMinutes(0),s.setSeconds(0),s.setMilliseconds(0);var o=e.duration_number;ds=e.duration_scale;if(ds=="never"){ds=e.period_scale;}switch(ds){case"never":case"hour":case"hours":s.setTime(r+60*o*60*1e3);break;case"day":case"days":s.setTime(r+24*o*60*60*1e3);break;case"weekday":case"weekdays":var i=o%5,o=(o-i)/5*7+i;5<i+a.getDay()&&(o+=2),s.setTime(r+24*o*60*60*1e3);break;case"week":case"weeks":s.setTime(r+7*o*24*60*60*1e3);break;case"month":case"months":var n=o-12*(f=Math.floor(o/12));s.setFullYear(s.getFullYear()+f,s.getMonth()+n,s.getDate());break;case"year":case"years":s.setFullYear(s.getFullYear()+o,s.getMonth(),s.getDate())}Math.floor((s.getTime()-r)/864e5);var c=e.percent,l=e.compound,m=e.principal,d=e.principal_back,u=parseInt(e.bonus_amount);isNaN(u)&&(u=0);var g=m+="%"==e.bonus_worth?m*(u/100):u,p=m*(d/100),y=new Date;y.setHours(0),y.setMinutes(0),y.setSeconds(0),y.setMilliseconds(0),creinv.push({x:moment(y).format(timeFormat),y:g.toFixed(2)}),cprofit.push({x:moment(y).format(timeFormat),y:p.toFixed(2)});var h,b,F=!0,k=e.period_every;for(k<1&&(k=1);y.getTime()<s.getTime();){switch(e.period_scale){case"onmaturity":y=s;break;case"hour":case"hours":y.setTime(y.getTime()+60*k*60*1e3);break;case"day":case"days":y.setTime(y.getTime()+24*k*60*60*1e3);break;case"weekday":case"weekdays":y.setTime(y.getTime()+24*k*60*60*1e3),F=0!=y.getDay()&&6!=y.getDay();break;case"week":case"weeks":y.setTime(y.getTime()+7*k*24*60*60*1e3);break;case"month":case"months":var f,n=k-12*(f=Math.floor(k/12));y.setFullYear(y.getFullYear()+f,y.getMonth()+n,y.getDate());break;case"year":case"years":y.setFullYear(y.getFullYear()+k,y.getMonth(),y.getDate())}F&&(g+=b=l/100*(h=g*(c/100)),p+=h-b+d/100*b),creinv.push({x:moment(y).format(timeFormat),y:g.toFixed(2)}),cprofit.push({x:moment(y).format(timeFormat),y:p.toFixed(2)})}document.getElementById("chart_principal").innerHTML=m.toFixed(2),document.getElementById("chart_reinv").innerHTML=(g-m).toFixed(2),document.getElementById("chart_profit").innerHTML=p.toFixed(2),document.getElementById("chart_profit_percent").innerHTML=(p/m*100).toFixed(0),chartconfig.data.datasets[1].data=creinv,chartconfig.data.datasets[0].data=cprofit,window.myLine.update()}
